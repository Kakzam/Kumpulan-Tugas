<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kost.ku">

    <application
        android:allowBackup="true"
        android:icon="@drawable/kostku"
        android:label="@string/app_name"
        android:roundIcon="@drawable/kostku"
        android:supportsRtl="true"
        android:theme="@style/Theme.KostKu">
        <activity android:name=".KostInputActivity"/>
        <activity android:name=".InputActivity"/>
        <activity android:name=".KostActivity"/>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>